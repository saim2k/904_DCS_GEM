GetDP(dyn_string dps, dyn_bool values){

  bool MixtureProtection, FlowProtection;
  
  MixtureProtection = values[1];
  DebugTN(\"Mixture\", MixtureProtection);
  FlowProtection = values[2];
  DebugTN(\"Flow\", FlowProtection);
  
  
  if(values[1]){
    this.text(\"Detector Protection Fired! - Wrong gas mixture!\");
    this.foreCol(\"STD_value_not_ok\");
   }  
  else if(values[2]){
    this.text(\"Detector Protection Fired! - Low gas flow!\");
    this.foreCol(\"STD_value_not_ok\");
  }  
  else{
    this.text(\"Detector Protection not fired\");  
    this.foreCol(\"STD_value_ok\");
  }
  
