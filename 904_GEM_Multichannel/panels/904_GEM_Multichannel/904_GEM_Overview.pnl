main()
{

  dyn_string dps = makeDynString(\"CAEN/904_HV_mainframe/board12/channel006.actual.status:_online.._value\" , \"CAEN/904_HV_mainframe/board12/channel005.actual.status:_online.._value\" , \"CAEN/904_HV_mainframe/board12/channel004.actual.status:_online.._value\" , 
                                 \"CAEN/904_HV_mainframe/board12/channel003.actual.status:_online.._value\" , \"CAEN/904_HV_mainframe/board12/channel002.actual.status:_online.._value\" , \"CAEN/904_HV_mainframe/board12/channel001.actual.status:_online.._value\" ,
                                 \"CAEN/904_HV_mainframe/board12/channel000.actual.status:_online.._value\");
  
  dpConnect(\"GetStatus\",dps)  ;
  
 
}


bool checkAllDpsExist(dyn_string dps)
  {
    bool exist=1;
    int i=1;
    for (i=1; i<=dynlen(dps); i++)  exist=exist && dpExists(dps[i]);
    return exist;
  }


GetStatus(dyn_string dps, dyn_int results)
  {
    dyn_errClass err;
    dyn_bool OvC_bit, OvV_bit, UnV_bit, Trip_bit;

      if( !checkAllDpsExist(dps) )
    {
      setValue(\"\", \"color\", \"_dpdoesnotexist\");
      return;
    }
    
      for (int i=1;i<=dynlen(dps);i++) 
      {
      OvC_bit[i]=(results[i] & 0x008);
      OvV_bit[i]=(results[i] & 0x010); 
      UnV_bit[i]=(results[i] & 0x020);      
      Trip_bit[i]=(results[i] & 0x200);      
      }

    
    if(results[1]==0 && results[2]==0 && results[3]==0 && results[4]==0 && results[5]==0 && results[6]==0 && results[7]==0){
        setValue(\"\",\"backCol\",\"grey\");
      }  
    else if(results[1]==1 && results[2]==1 && results[3]==1 && results[4]==1 && results[5]==1 && results[6]==1 && results[7]==1){ 
        setValue(\"\",\"backCol\",\"STD_led_On\");
      }
    else if(OvC_bit[1]==1 || OvC_bit[2]==1 || OvC_bit[3]==1 || OvC_bit[4]==1 || OvC_bit[5]==1 || OvC_bit[6]==1 || OvC_bit[7]==1 || OvV_bit[1]==1 || OvV_bit[2]==1 || OvV_bit[3]==1 || OvV_bit[4]==1 || OvV_bit[5]==1 || OvV_bit[6]==1 || OvV_bit[7]==1 || UnV_bit[1]==1 || UnV_bit[2]==1 || UnV_bit[3]==1 || UnV_bit[4]==1 || UnV_bit[5]==1 || UnV_bit[6]==1 || UnV_bit[7]==1 || Trip_bit[1]==1 || Trip_bit[2]==1 || Trip_bit[3]==1 || Trip_bit[4]==1 || Trip_bit[5]==1 || Trip_bit[6]==1 || Trip_bit[7]==1)
    {
        setValue(\"\",\"backCol\",\"red\");
    }
    else {
        setValue(\"\",\"backCol\",\"STD_value_light\");
      }
  }




