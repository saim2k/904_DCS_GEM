void disableAlarms(dyn_string channelsDpList){

      DebugTN(\"complete channel list = \", channelsDpList);   
      
      
      dyn_string dpListFinal;
      dyn_string dp;
      
      dyn_string alarmList;
      alarmList = selectionList_alarms.items;

      bool ok;
      dyn_string error;

      if(dynlen(alarmList)==0){
          fwGeneral_openMessagePanel(\"Error! No alarms selected!\", ok, error, \"Info\", TRUE);            
      }
      else{
        for(int w=1; w<dynlen(channelsDpList)+1; w++){
          if(alarmList[1]==\"All\" ){
            dp = makeDynString(channelsDpList[w]+\".actual.Trip\",
                               channelsDpList[w]+\".actual.OvC\",
                               channelsDpList[w]+\".actual.OvV\",
                               channelsDpList[w]+\".actual.UnV\",
                               channelsDpList[w]+\".actual.externalDisable\",
                               channelsDpList[w]+\".actual.status\");
                             
            dynAppend(dpListFinal, dp);         
          }
          else{
            for(int x=1; x<dynlen(alarmList)+1; x++){
              dp[x] = channelsDpList[w]+\".\"+alarmList[x];
              dynAppend(dpListFinal, dp[x]);
            }
          }
        }
      }      
      
      dyn_string Info;      
      
        fwAlertConfig_deactivateMultiple(dpListFinal, Info, TRUE, FALSE, FALSE);         
        
        DebugTN(\"exceptionInfo: \", Info);

        DebugTN(\"final list: \", dpListFinal);


