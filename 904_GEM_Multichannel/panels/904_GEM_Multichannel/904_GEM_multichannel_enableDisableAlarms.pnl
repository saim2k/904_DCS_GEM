main(mapping event)
{
  dyn_string chambers;
  chambers = selectionList_chambers.items;
  
  dyn_string channels;
  channels = selectionList_channels.items;
  
  dyn_string alarms;
  alarms = selectionList_alarms.items;

  
  dyn_string exceptionInfo;
  bool answ;
  
    string message;
    message = \"You have selected the chambers: \" + chambers + \". In each chamber, the alarms will be disabled for channels: \" + channels + \". The alarms that will be disabled are: \" + alarms + \". Are you sure?\";
    fwGeneral_openMessagePanel(message, answ, exceptionInfo, \"Confirm\", false);  
  
    if(answ){
      
      getChambers(chambers);
    } 
  
  
}

