void getChannelsTop(string dp){ 

  dyn_string channelList;
  channelList = selectionList_channels.items;
  DebugTN(\"channelList = \", channelList, dynlen(channelList));
  
  dyn_string completeChannelList;
  dyn_string channelDp;

  bool ok;
  dyn_string error;
 
        if(dynlen(channelList)==0){
              fwGeneral_openMessagePanel(\"Error! No channels selected!\", ok, error, \"Info\", TRUE);            
        }
        else{
          for(int j=1; j<dynlen(channelList)+1; j++){
            if(channelList[j]==\"Drift\"){
              channelDp[j] = dp+\"channel006\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G1Top\"){
              channelDp[j] = dp+\"channel005\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G1Bot\"){
              channelDp[j] = dp+\"channel004\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G2Top\"){
              channelDp[j] = dp+\"channel003\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G2Bot\"){
              channelDp[j] = dp+\"channel002\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G3Top\"){
              channelDp[j] = dp+\"channel001\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"G3Bot\"){
              channelDp[j] = dp+\"channel000\";                          
              dynAppend(completeChannelList, channelDp[j]);
            }
            else if(channelList[j]==\"All\"){
              channelDp = makeDynString(dp+\"channel000\", 
                                        dp+\"channel001\", 
                                        dp+\"channel002\", 
                                        dp+\"channel003\", 
                                        dp+\"channel004\", 
                                        dp+\"channel005\", 
                                        dp+\"channel006\");                          
              dynAppend(completeChannelList, channelDp);
            }
          }
        }
          
  disableAlarms(completeChannelList);
}

          
