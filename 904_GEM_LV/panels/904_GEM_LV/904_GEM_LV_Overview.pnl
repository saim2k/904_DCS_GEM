main()
{

  string dps = makeDynString(\"cms_gem_dcs_1:CAEN/904_Shared_mainframe/branchController00/easyCrate0/easyBoard16/channel000.actual.status\");
  
  dpConnect(\"GetStatus\",dps)  ;
  
 
}


bool checkAllDpsExist(string dps)
  {
    bool exist=1;
    exist=exist && dpExists(dps);
    return exist;
  }


GetStatus(string dps, int results)
  {
    dyn_errClass err;
    bool OvC_bit, OvV_bit, UnV_bit, Trip_bit, HVMax_bit, CalErr_bit, ComErr_bit, OVP_bit, PWF_bit, TErr_bit;
// 
       if( !checkAllDpsExist(dps) )
     {
       setValue(\"\", \"color\", \"_dpdoesnotexist\");
       return;
     }

       OvC_bit=(results & 0x008);
       OvV_bit=(results & 0x010); 
       UnV_bit=(results & 0x020);      
       Trip_bit=(results & 0x200);  
       HVMax_bit=(results & 0x080);
       CalErr_bit=(results & 0x400); 
       ComErr_bit=(results & 0x800); 
       OVP_bit=(results & 0x2000); 
       PWF_bit=(results & 0x4000);      
       TErr_bit=(results & 0x8000);  
  
//     DebugTN(\"Trip = \", Trip_bit );       
   
    
    if(results==0){
        setValue(\"\",\"backCol\",\"grey\");
      }  
    else if(OvC_bit==TRUE || OvV_bit==TRUE || UnV_bit==TRUE || Trip_bit==TRUE || HVMax_bit==TRUE || CalErr_bit==TRUE || ComErr_bit==TRUE || OVP_bit==TRUE || PWF_bit==TRUE || TErr_bit==TRUE){
      setValue(\"\",\"backCol\",\"red\");
      }
    else if(results==1){ 
        setValue(\"\",\"backCol\",\"STD_led_On\");
      }
    else{
        setValue(\"\",\"backCol\", \"yellow\");    
    
    }
  }




